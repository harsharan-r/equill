{
	"name": "Equill",
	"version": "1.0",
	"manifest_version": 3,
	"description": "Test",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"css": [],
			"run_at": "document_start"
		}
	],
	"permissions": ["contextMenus", "scripting", "activeTab", "storage", "sidePanel"],
	"side_panel": {
		"default_path": "main.html"
	},
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	},
	"action": {
		"default_title": "Open panel"
	}
}
